<?xml version="1.0" encoding="utf-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/19.0">
    <id>com.tuarua.VibrationANE</id>
    <name>Vibration ANE</name>
    <copyright>This work is licensed under Apache License, Copyright (c) 2018 Tua Rua Ltd.</copyright>
    <versionNumber>1.1.0</versionNumber>
    <platforms>
        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libVibrationANE.a</nativeLibrary>
                <initializer>TRVIBExtInizer</initializer>
                <finalizer>TRVIBExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="iPhone-x86">
            <applicationDeployment>
                <nativeLibrary>libVibrationANE.a</nativeLibrary>
                <initializer>TRVIBExtInizer</initializer>
                <finalizer>TRVIBExtFinizer</finalizer>
            </applicationDeployment>
        </platform>
    </platforms>
</extension>
